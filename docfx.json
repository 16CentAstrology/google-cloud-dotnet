{
  "metadata": [
    {
      "src": [
        {
          "files": [ "src/*/project.json" ],
          "exclude": [ "**/bin/**", "**/obj/**" ],
          "cwd": ""
        }
      ],
      "dest": "obj/api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "**/*.yml" ],
        "cwd": "obj/api",
        "dest": "api"
      },
      {
        "files": [ "*.md", "toc.yml" ],
        "cwd": "docs"
      }
    ],
    "globalMetadata": {
      "_appTitle": "Google Cloud APIs"
    },
    "dest": "_site"
  }
}
