{
  "metadata": [
    {
      "src": [
        {
          "files": [ "*/project.json" ],
          "exclude": [ "**/bin/**", "**/obj/**" ],
          "cwd": "../src"
        }
      ],
      "dest": "obj/api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "**/*.yml" ],
        "src": "obj/api",
        "dest": "api"
      },
      {
        "files": [ "index.md", "toc.yml", "articles/*.md" ],
      }
    ],
    "globalMetadata": {
      "_appTitle": "Google Cloud APIs",
      "_disableContribution": true
    },
    "overwrite": [ "extra/*.md", "obj/snippets/*.md" ],
    "dest": "_site"
  }
}
